        };

        adc {
                ti,adc-channels = <0 1 2 3>;
        };
};

&dcan0 {
        pinctrl-names = "default";
        pinctrl-0 = <&dcan0_default>;
        status = "okay";
};

&sham {
        status = "okay";
};

&aes {
        status = "okay";
};
